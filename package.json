{
  "name": "sapper-tailwind-apollo-storybook",
  "description": "sapper + tailwind + apollo + storybook - Starter Template",
  "version": "0.0.1",
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "devbuild:tailwind": "NODE_ENV=development postcss static/tailwind.css -o static/index.css",
    "dev": "sapper dev",
    "build": "npm run build:tailwind && sapper build",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "apollo-boost": "^0.4.9",
    "compression": "^1.7.1",
    "graphql": "^15.1.0",
    "node-fetch": "^2.6.1",
    "polka": "next",
    "sirv": "^0.4.0",
    "svelte-apollo": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/svelte": "^5.3.19",
    "babel-loader": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "svelte": "^3.23.2",
    "svelte-loader": "^2.13.6",
    "tailwindcss": "^1.4.6"
  }
}
